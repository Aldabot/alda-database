type User {
  id: ID! @unique
  psid: String! @unique
  saltedgeCustomer: SaltedgeCustomer
}

type SaltedgeCustomer {
  id: ID! @unique
  customerId: String!
  user: User!
  logins: [SaltedgeLogin!]!
}

type SaltedgeLogin {
  id: ID! @unique
  loginId: String! @unique
  accounts: [SaltedgeAccount!]!
}

type SaltedgeAccount {
  id: ID! @unique
  accountId: String! @unique
  balance: Float
}
